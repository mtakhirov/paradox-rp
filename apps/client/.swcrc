{
  "$schema": "https://json.schemastore.org/swcrc",

  "jsc": {
    "parser": {
      "syntax": "typescript",
      "dynamicImport": true,
      "decorators": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "loose": false,
    "preserveAllComments": false,
    "target": "esnext"
    // "loose": true,
    // "minify": {
    //   "compress": {
    //     "drop_console": true
    //   },
    //   "mangle": true
    // }
  },
  "module": {
    "strict": true,
    "strictMode": true,
    "noInterop": true,
    "type": "commonjs"
  }
}
